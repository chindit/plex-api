{
    "name": "chindit/plex-api",
    "description": "A simple API library to interact with a Plex server",
    "type": "library",
    "version": "2.1.2",
    "require": {
        "php": "^8.3",
        "ext-simplexml": "*",
        "chindit/collection": "^1.0",
        "symfony/http-client": "^7.3"
    },
    "require-dev": {
        "pestphp/pest": "^4.1",
        "phpstan/phpstan": "^2.1"
    },
    "license": "GPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "Chindit\\PlexApi\\": "src/"
        }
    },
    "authors": [
        {
            "name": "David Lumaye",
            "email": "littletiger58@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
